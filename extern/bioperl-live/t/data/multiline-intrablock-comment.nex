#NEXUS

BEGIN TAXA;
[block-level 
comment 
in 
the 
TAXA 
block]
      dimensions ntax=8;
      taxlabels A B C D E F G H;  
END;

BEGIN CHARACTERS;
[block-level comment 
in the 
CHARACTERS block]

      dimensions nchar=5;
      format datatype=protein missing=? gap=-;
      charlabels 1 2 3 4 5;
      matrix
A     --ONE
B     --ONE
C     TWO--
D     THREE
E     F-OUR
F     FIVE-
G     SIX--
H     SEVEN;
END;

BEGIN TREES;
[block-level 
comment 
                                  in the TREES block]

       tree ladder = (((((((A:1,B:1):1,C:2):1,D:3):1,E:4):1,F:5):1,G:6):1,H:7);
END;